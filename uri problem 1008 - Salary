#include <stdio.h>

int main()
{   
   int number, hours;
   float amount, salary;
 
    scanf("%d%d",&number,&hours);
    
     scanf("%f",&amount);
         
         salary = hours*amount;
         
         printf("NUMBER = %d\n",number);
         printf("SALARY = U$ %0.2f\n",salary);

    return 0;
}